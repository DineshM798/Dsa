import pandas as pd
#sample  data
data={'Scores':[85,90,78,92,88,76,95,89,84,91}
df=pd.DataFrame(data)
#Frequency distribution 
freq_dist=df['score'].value_counts()
Print("Frequency Distribution:\n",freq_dist)
#Calculating averages and variability
Print("Mean:",df['Scores'].mean())
Print("Standard Deviation:",df['Scores'].std())
